{
    "name": "mia/statamic-image-renderer",
    "keywords": [
        "statamic",
        "responsive",
        "images",
        "imgix",
        "glide"
    ],
    "homepage": "https://github.com/moodley-interactive/statamic-responsive-image",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Mia\\ImageRenderer\\": "src",
			"League\\ColorExtractor\\": "vendor/league/color-extractor/src/League/ColorExtractor/"
        }
    },
    "authors": [
        {
            "name": "moodley interactive"
        }
    ],
    "extra": {
        "statamic": {
            "name": "Responsive Image Addon",
            "description": "Generates art directed picture elements. Works with Glide and Imgix."
        },
        "laravel": {
            "providers": [
                "Mia\\ImageRenderer\\ServiceProvider"
            ]
        }
    },
    "require": {
        "bepsvpt/blurhash": "^1.0",
        "league/color-extractor": "0.3.3",
        "ozdemirburak/iris": "^2.3"
    },
    "require-dev": {
        "spatie/laravel-ray": "^1.17"
    },
	"repositories": [
        {
            "type": "package",
            "package": {
                "name": "league/color-extractor",
                "version": "0.3.3",
                "source": {
                    "url": "https://github.com/Mihairu35/color-extractor.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        }
    ]
}
